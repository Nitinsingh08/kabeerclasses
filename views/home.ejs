<section class="container px-4 mt-5 mb-3 mb-lg-5 mb-xl-5 mb-md-5 pb-5">
  <h2
    class="text-center mb-5 pb-3 pb-lg-5 pb-xl-5 pb-md-5 font-weight-600 navy-blue"
  >
    Let Us Guide You Towards Success
  </h2>
  <div class="row gx-5 justify-content-evenly">
    <div class="col-lg-6">
      <form
        class="row needs-validation"
        action="https://formsubmit.co/83666bcdf5eaf3fbc44edceb01b54ad5"
        method="POST"
        required
      >
        <div class="col-lg-6">
          <label
            for="validationCustom01"
            class="form-label mb-1 font-weight-500"
            >First Name</label
          >
          <input type="hidden" name="_next" value="/submit" />
          <input type="hidden" name="_subject" value="You got a new query !!" />
          <input
            name="first-Name"
            type="text"
            class="form-control rounded-1"
            placeholder="John"
            aria-label="First name"
            id="validationCustom01"
            required
          />
          <div class="valid-feedback">Looks good!</div>
        </div>
        <div class="col-lg-6">
          <label for="LastName" class="form-label mb-1 font-weight-500"
            >Last Name</label
          >
          <input
            name="last-Name"
            type="text"
            class="form-control rounded-1"
            placeholder="Doe"
            aria-label="Last name"
            required
          />
          <div class="valid-feedback">Looks good!</div>
        </div>
        <div class="col-lg-6 mt-3">
          <label for="Email" class="form-label mb-1 font-weight-500"
            >Email</label
          >
          <input
            name="Email address"
            type="email"
            class="form-control rounded-1"
            placeholder="johndoe@gmail.com"
            aria-label="email address"
            pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,4}$"
            required
          />
        </div>
        <div class="col-lg-6 mt-3">
          <label for="ContactNumber" class="form-label mb-1 font-weight-500"
            >Contact Number</label
          >
          <input
            name="Contact-number"
            type="tel"
            class="form-control rounded-1"
            placeholder="8555848014"
            aria-label="Contact Number"
            id="ContactNumber"
            pattern="[789][0-9]{9}"
            required
          />
        </div>
        <div class="col mt-3">
          <label
            for="exampleFormControlTextarea1"
            class="form-label mb-1 font-weight-500"
            >Write Your Message</label
          >
          <textarea
            name="Message"
            class="form-control rounded-1"
            id="exampleFormControlTextarea1"
            rows="4"
            placeholder="Ask your queries here..."
            required
          ></textarea>
        </div>

        <div class="col-12 text-lg-end text-xl-end text-start mt-3">
          <button type="submit" class="btn btn-primary btn-md">
            Send Message
          </button>
        </div>
      </form>
    </div>
    <div class="col-lg-6 form-illustration-coloumn">
      <img
        height="427"
        width="640"
        class="img-fluid img-responsive"
        src="/assets/college entrance exam-bro (1) 1.svg"
        alt=""
        srcset=""
      />
    </div>
  </div>
</section>

