<%- include("partials/header") %>
<div class="container mx-1">
  <div class="row justify-content-start">
    <div
      class="tabs col-12 col-md-6 col-xl-6 col-lg-6 d-flex flex-column align-items-stretch flex-shrink-0 bg-white"
    >
      <a
        href=""
        class="d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom"
      >
        <img
          height="36px"
          width="36px"
          src="data:image/png;base64,"
        />
        <span class="display-5 fs-5 fw-semibold ps-1 navy-blue"
          >List of Chapters</span
        >
      </a>
      <div
        class="tabs-sidebar list-group list-group-flush border-bottom scrollarea"
      >
        <!-- <a
          data-for-tab="1"
          href=""
          class="tabs-button list-group-item list-group-item-action py-3 lh-tight"
          aria-current="true"
        >
          <div class="d-flex w-100 align-items-center justify-content-between">
            <strong class="mb-1">This is the name of the chapter </strong>

            <small>Wed</small>
          </div>
          <div class="col-10 mb-1 small">
            Some placeholder content in a paragraph below the heading and date.
          </div>
        </a> -->

        <% for( i = 0; i < eval(link).length; i++) { %>
        <button
          data-tab-target="#<%= eval(link)[i].ChapterId %>"
          class="chapter-button list-group-item list-group-item-action py-3 lh-tight"
        >
          <div class="d-flex w-100 align-items-center justify-content-between">
            <strong class="mb-1 font-weight-600"
              ><%= eval(link)[i].NameOfChapter %></strong
            >
          </div>
          <div class="col-12 mb-1 small">
            <p>Some placeholder content in a paragraph below the heading.</p>
          </div>
        </button>
        <% } %>
      </div>
    </div>

    <div class="col-12 col-md-6 col-xl-6 col-lg-6 gx-5">
      <!-- <nav aria-label="breadcrumb" class="nav-breadcrumb border-0 pt-4">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item">Class 11th</li>
        </ol>
      </nav> -->
      <% for( i = 0; i < eval(link).length;i++) { %>
      <div
        data-tab-content
        id="<%= eval(link)[i].ChapterId %>"
        class="card lecture-container flex-stretch mt-3 align-self-center"
      >
        <div class="card-header">
          <h4 class="text-start navy-blue font-weight-600">
            <%= eval(link)[i].NameOfChapter %>
          </h4>
        </div>
        <div class="card-body d-flex text-start">
          <iframe
            width="870px"
            height="315px"
            src="<%= eval(link)[i].Lecture %>"
            title="YouTube video player"
            frameborder="0"
            allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
            allowfullscreen
          ></iframe>
        </div>
        <div class="card-footer pt-4">
          <h5 class="font-weight-600">Practice Sheet</h5>
          <div class="row">
            <p class="col-9">
              Download these practice sheets and cement the knowledge in your
              mind
            </p>
            <form class="float-end col-3 text-end" action="/download-file">
              <button class="pdf-download-btn btn btn-primary" type="submit">
                Download pdf
              </button>
            </form>
          </div>

          <h5 class="font-weight-600">Hand written notes</h5>
          <div class="row">
            <p class="col-9">
              Download these hand written notes from this chapter
            </p>
            <form class="float-end col-3 text-end" action="/download-file">
              <button class="pdf-download-btn btn btn-primary" type="submit">
                Download pdf
              </button>
            </form>
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
</div>

<!--CREATING TABS -->
<script>
  const tabs = document.querySelectorAll("[data-tab-target]");
  const tabContents = document.querySelectorAll("[data-tab-content]");
  tabs.forEach((tab) => {
    tab.addEventListener("click", () => {
      const target = document.querySelector(tab.dataset.tabTarget);
      tabContents.forEach((tabContent) => tabContent.classList.remove("activ"));
      target.classList.add("activ");
    });
  });
</script>
<%- include("partials/footer") %>
